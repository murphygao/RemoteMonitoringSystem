<Window x:Class="RMS.Agent.OutOfServiceApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Out of Service" Height="768" Width="1024" Background="#FF2E6BC3" WindowStartupLocation="CenterScreen" WindowState="Maximized" WindowStyle="None">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Resources/OOS_CONTEXT.png" Stretch="None"/>
        </Grid.Background>
        <RepeatButton x:Name="btnMenu" DockPanel.Dock="Top" Delay="5000" Interval="1000" Content="Menu" Click="btnMenu_Click" HorizontalAlignment="Right" Width="224" Height="142" VerticalAlignment="Top" PreviewMouseLeftButtonUp="btnMenu_PreviewMouseLeftButtonUp" Background="{x:Null}" Opacity="0.2"/>
    </Grid>
</Window>
