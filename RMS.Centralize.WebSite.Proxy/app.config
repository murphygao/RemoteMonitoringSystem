<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IMessageActionService" />
                <binding name="BasicHttpBinding_IClientService" />
                <binding name="BasicHttpBinding_ISummaryReportService" />
                <binding name="BasicHttpBinding_IMonitoringService" />
                <binding name="BasicHttpBinding_ISelfTestingService" />
                <binding name="BasicHttpBinding_IActionProfileService" />
                <binding name="BasicHttpsBinding_IActionProfileService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpsBinding_IClientService">
                    <security mode="Transport" />
                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost/RMS.Centralize.WebService/SummaryReportService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISummaryReportService"
                contract="SummaryReportProxy.ISummaryReportService" name="BasicHttpBinding_ISummaryReportService" />
            <endpoint address="http://localhost/RMS.Centralize.WebService/MonitoringService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMonitoringService"
                contract="MonitoringProxy.IMonitoringService" name="BasicHttpBinding_IMonitoringService" />
            <endpoint address="http://localhost/RMS.Centralize.WebService/SelfTestingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISelfTestingService"
                contract="SelfTestingProxy.ISelfTestingService" name="BasicHttpBinding_ISelfTestingService" />
            <endpoint address="http://esn-nb01/RMS.Centralize.WebService/ActionProfileService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IActionProfileService"
                contract="ActionProfileProxy.IActionProfileService" name="BasicHttpBinding_IActionProfileService" />
            <endpoint address="https://esn-nb01/RMS.Centralize.WebService/ActionProfileService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_IActionProfileService"
                contract="ActionProfileProxy.IActionProfileService" name="BasicHttpsBinding_IActionProfileService" />
            <endpoint address="http://esn-nb01/RMS.Centralize.WebService/ClientService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IClientService"
                contract="ClientProxy.IClientService" name="BasicHttpBinding_IClientService" />
            <endpoint address="https://esn-nb01/RMS.Centralize.WebService/ClientService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpsBinding_IClientService"
                contract="ClientProxy.IClientService" name="BasicHttpsBinding_IClientService" />
        </client>
    </system.serviceModel>
</configuration>